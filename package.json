{
   "name": "currency-exchange-api",
   "version": "0.0.1",
   "description": "Currency Exchange API",
   "type": "module",
   "scripts": {
      "dev": "node --watch --loader ts-node/esm --no-warnings src/index.ts",
      "build": "rimraf dist && tsc",
      "start": "node dist/index.js",
      "db:setup": "docker-compose up -d",
      "db:migration:run": "npx typeorm-ts-node-esm migration:run -d ./src/data-source.ts",
      "test": "NODE_ENV=TEST vitest",
      "test:coverage": "NODE_ENV=TEST vitest --coverage",
      "lint": "eslint . --ext .ts --ignore-pattern '**/*.test.ts'"
   },
   "license": "MIT",
   "author": "Julius Grybauskas",
   "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^20.9.4",
      "@types/swagger-ui-express": "^4.1.7",
      "@types/yamljs": "^0.2.34",
      "@typescript-eslint/eslint-plugin": "^6.13.1",
      "@typescript-eslint/parser": "^6.12.0",
      "@vitest/coverage-v8": "^2.1.8",
      "eslint": "^8.55.0",
      "eslint-config-standard-with-typescript": "^40.0.0",
      "eslint-plugin-import": "^2.29.0",
      "eslint-plugin-n": "^16.3.1",
      "eslint-plugin-promise": "^6.1.1",
      "rimraf": "^6.0.1",
      "ts-node": "^10.9.1",
      "typescript": "^5.3.2",
      "vitest": "^2.1.8"
   },
   "dependencies": {
      "@types/jsonfile": "^5.0.0",
      "chalk": "^4.0.0",
      "class-validator": "^0.14.0",
      "compression": "^1.7.5",
      "cors": "^2.8.5",
      "cron": "^3.2.1",
      "express": "^4.17.1",
      "express-openapi": "^12.1.3",
      "express-openapi-validator": "^5.3.9",
      "express-rate-limit": "^7.4.1",
      "express-validator": "^7.0.1",
      "fastq": "^1.17.1",
      "helmet": "^8.0.0",
      "jsonfile": "^6.1.0",
      "p-retry": "^6.2.1",
      "pg": "^8.11.3",
      "pino": "^9.5.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^5.0.1",
      "typeorm": "^0.3.17",
      "validator-js": "^0.2.1",
      "yamljs": "^0.3.0",
      "zod": "^3.23.8"
   }
}
